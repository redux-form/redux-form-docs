{
  "name": "redux-form-website",
  "version": "0.0.1",
  "description": "The generator for the redux-form website",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/redux-form"
  },
  "scripts": {
    "build": "BABEL_ENV=es babel src --out-dir lib",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run lint && npm run build",
    "publish": "node lib/publish.js",
    "postinstall": "mv node_modules/redux-form node_modules/ReduxForm && mv node_modules/react-dom node_modules/ReactDOM"
  },
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikras/redux-form/issues"
  },
  "homepage": "https://redux-form.com",
  "dependencies": {
    "http-client": "^4.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.forin": "^4.0.1",
    "mkdirp": "^0.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "redux-form": "^8.0.4",
    "redux-form-doc-version-checker": "^1.0.13",
    "redux-form-website-template": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.3.15",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.9.0"
  },
  "npmName": "redux-form-website-generator",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
