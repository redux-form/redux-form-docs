{
  "name": "redux-form-examples",
  "description": "A showcase and testbed for redux-form",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/redux-form"
  },
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "dev": "node ./node_modules/better-npm-run dev",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm test && npm run build",
    "publish": "npm run prepublish && git add -f dist/* && git commit -m \"publishing\" && git push origin gh-pages",
    "test": "mocha --compilers js:babel-register --recursive 'src/**/__tests__/*'"
  },
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "redux",
    "react-redux",
    "redux-form",
    "form",
    "decorator",
    "example",
    "demo"
  ],
  "betterScripts": {
    "dev": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_ENV": "development",
        "NODE_PATH": "./src"
      }
    }
  },
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikras/redux-form/issues"
  },
  "homepage": "https://github.com/erikras/redux-form",
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "eslint-loader": "^1.3.0",
    "history": "^2.0.1",
    "html-loader": "^0.4.3",
    "markdown-loader": "^0.1.7",
    "raw-loader": "^0.5.1",
    "react-addons-perf": "^15.0.0",
    "react-bootstrap": "^0.28.5",
    "react-router": "^2.0.1",
    "react-router-bootstrap": "^0.20.1",
    "redux-form": "5.3.6",
    "scroll-behavior": "^0.9.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "eslint": "^4.18.2",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.16.0",
    "express": "^4.18.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "less": "^2.6.1",
    "mocha": "^10.2.0",
    "node-sass": "^3.4.2",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "redux": "^3.3.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "rifraf": "^2.0.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "serve-static": "^1.15.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
